{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts", "src/testing.ts"],
  "out": "docs-dist/api",
  "plugin": [],
  "name": "Leaflet-Node API Documentation",
  "includeVersion": true,
  "exclude": ["**/node_modules/**", "**/dist/**", "**/tests/**"],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "readme": "none",
  "navigationLinks": {
    "Examples": "../index.html",
    "GitHub": "https://github.com/jburnhams/leaflet-node",
    "npm": "https://www.npmjs.com/package/leaflet-node"
  },
  "sort": ["source-order", "required-first", "kind"],
  "kindSortOrder": [
    "Function",
    "Class",
    "Interface",
    "TypeAlias",
    "Variable",
    "Enum"
  ],
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  }
}
